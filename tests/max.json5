{
  "id": "max",
  "tests": [
    {
      "arguments": {
        "data": [
          1,
          0,
          3,
          2
        ]
      },
      "returns": 3
    },
    {
      "arguments": {
        "data": [
          5,
          2.5,
          null,
          -0.7
        ]
      },
      "returns": 5
    },
    {
      "arguments": {
        "data": [
          1,
          0,
          3,
          null,
          2
        ],
        "ignore_nodata": false
      },
      "returns": null
    },
    {
      "arguments": {
        "data": [
          NaN,
          1,
          2,
          3
        ]
      },
      "returns": NaN
    },
    {
      "arguments": {
        "data": [
          1,
          2,
          Infinity,
          3
        ]
      },
      "returns": Infinity
    },
    {
      "arguments": {
        "data": [
          1,
          -Infinity,
          2,
          3
        ]
      },
      "returns": 3
    },
    {
      "arguments": {
        "data": [
          1.12345678901,
          1.12345678903,
          1.12345678902
        ]
      },
      "returns": 1.12345678903
    },
    {
      // The input array is empty: return `null`.
      "arguments": {
        "data": []
      },
      "returns": null
    }
  ]
}
