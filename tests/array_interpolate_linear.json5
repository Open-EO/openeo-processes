{
  "id": "array_interpolate_linear",
  "tests": [
    {
      "arguments": {
        "data": [
          null,
          1,
          null,
          6,
          null,
          -8,
        ]
      },
      "returns": [
        null,
        1,
        3.5,
        6,
        -1,
        -8
      ]
    },
    {
      "arguments": {
        "data": [
          1,
          NaN,
          6,
          NaN,
          -8,
          NaN,
        ]
      },
      "returns": [
        1,
        3.5,
        6,
        -1,
        -8,
        NaN
      ]
    },
    {
      "arguments": {
        "data": [
          -Infinity,
          NaN,
          0.1,
          NaN,
          0.2,
          NaN,
          Infinity
        ]
      },
      "returns": [
        -Infinity,
        -Infinity,
        0.1,
        0.15,
        0.2,
        Infinity,
        Infinity
      ]
    },
    {
      "arguments": {
        "data": [
          null,
          1,
          null,
          null
        ]
      },
      "returns": [
        null,
        1,
        null,
        null
      ]
    },
    {
      "arguments": {
        "data": [
          null,
          null,
          null
        ]
      },
      "returns": [
        null,
        null,
        null
      ]
    },
    {
      "arguments": {
        "data": {
          "type": "labeled-array",
          "data": [
            {
              "key": "a",
              "value": 1.0
            },
            {
              "key": "b",
              "value": NaN
            },
            {
              "key": "c",
              "value": 2.0
            }
          ]
        }
      },
      "returns": {
        "type": "labeled-array",
        "data": [
          {
            "key": "a",
            "value": 1.0
          },
          {
            "key": "b",
            "value": 1.5
          },
          {
            "key": "c",
            "value": 2.0
          }
        ]
      }
    },
    {
      "arguments": {
        "data": {
          "type": "labeled-array",
          "data": [
            {
              "key": 1,
              "value": 1.0
            },
            {
              "key": 3,
              "value": null
            },
            {
              "key": 4,
              "value": 4.0
            }
          ]
        }
      },
      "returns": {
        "type": "labeled-array",
        "data": [
          {
            "key": 1,
            "value": 1.0
          },
          {
            "key": 3,
            "value": 3.0
          },
          {
            "key": 4,
            "value": 4.0
          }
        ]
      }
    }
  ]
}