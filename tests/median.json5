{
  "id": "median",
  "level": "L1",
  "tests": [
    {
      "arguments": {
        "data": [
          1,
          3,
          3,
          6,
          7,
          8,
          9
        ]
      },
      "returns": 6
    },
    {
      "arguments": {
        "data": [
          1,
          2,
          3,
          4,
          5,
          6,
          8,
          9
        ]
      },
      "returns": 4.5
    },
    {
      "arguments": {
        "data": [
          8,
          2,
          5,
          9,
          3,
          6,
          1,
          4
        ]
      },
      "returns": 4.5
    },
    {
      "arguments": {
        "data": [
          -0.1,
          0,
          0.56,
          -0.76,
          0.87,
          -0.237
        ]
      },
      "returns": -0.05
    },
    {
      "arguments": {
        "data": [
          -Infinity,
          3,
          3,
          5,
          Infinity
        ]
      },
      "returns": 3
    },
    {
      "arguments": {
        "data": [
          NaN,
          3,
          3,
          NaN,
          5
        ]
      },
      "returns": NaN
    },
    {
      "arguments": {
        "data": [
          -1,
          -0.5,
          {"type": "nodata"},
          1
        ]
      },
      "returns": -0.5
    },
    {
      "arguments": {
        "data": [
          -1,
          0,
          {"type": "nodata"},
          1
        ],
        "ignore_nodata": false
      },
      "returns": {"type": "nodata"}
    },
    {
      // The input array is empty: return `{"type": "nodata"}`.
      "arguments": {
        "data": []
      },
      "returns": {"type": "nodata"}
    },
    {
      // The input array has only `{"type": "nodata"}` elements: return `{"type": "nodata"}`.
      "arguments": {
        "data": [
          {"type": "nodata"},
          {"type": "nodata"}
        ]
      },
      "returns": {"type": "nodata"}
    }
  ]
}
