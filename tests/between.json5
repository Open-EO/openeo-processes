{
  "tests": [
    {
      "arguments": {
        "x": null,
        "min": 0,
        "max": 1
      },
      "returns": null
    },
    {
      "arguments": {
        "x": 1,
        "min": 0,
        "max": 1
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 1,
        "min": 0,
        "max": 1,
        "exclude_max": true
      },
      "returns": false
    },
    {
      // Swapped bounds (min is greater than max) MUST always return `false`
      "arguments": {
        "x": 0.5,
        "min": 1,
        "max": 0
      },
      "returns": false
    },
    {
      "arguments": {
        "x": -0.5,
        "min": -1,
        "max": 0
      },
      "returns": true
    }
  ]
}