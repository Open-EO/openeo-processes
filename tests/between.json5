{
  "id": "between",
  "level": "L1",
  "tests": [
    {
      "arguments": {
        "x": null,
        "min": 0,
        "max": 1
      },
      "returns": null
    },
    {
      "arguments": {
        "x": "0.5",
        "min": 0,
        "max": 1
      },
      "returns": false
    },
    {
      "arguments": {
        "x": true,
        "min": 0,
        "max": 1
      },
      "returns": false
    },
    {
      "arguments": {
        "x": false,
        "min": 0,
        "max": 1
      },
      "returns": false
    },
    {
      "arguments": {
        "x": 1.0,
        "min": 0.0,
        "max": 1.0
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 1,
        "min": 0,
        "max": 1
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 1,
        "min": 0,
        "max": 1,
        "exclude_max": true
      },
      "returns": false
    },
    {
      "arguments": {
        "x": 0,
        "min": 0,
        "max": 1,
        "exclude_max": true
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 0.99999999999,
        "min": 0.0,
        "max": 1.0,
        "exclude_max": true
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 1.234567891,
        "min": 1.234567890,
        "max": 1.234567892
      },
      "returns": true
    },
    {
      "arguments": {
        "x": 1.23456789,
        "min": 1.23456789,
        "max": 1.23456789
      },
      "returns": true
    },
    {
      // Swapped bounds (min is greater than max) MUST always return `false`
      "arguments": {
        "x": 0.5,
        "min": 1,
        "max": 0
      },
      "returns": false
    },
    {
      "arguments": {
        "x": -0.5,
        "min": -1,
        "max": 0
      },
      "returns": true
    },
    {
      "arguments": {
        "x": [1, 2, 3],
        "min": 0,
        "max": 4
      },
      "returns": false
    },
  ]
}
