{
  "id": "mean",
  "tests": [
    {
      "arguments": {
        "data": [
          1,
          0,
          3,
          2
        ]
      },
      "returns": 1.5
    },
    {
      "arguments": {
        "data": [
          9,
          2.5,
          null,
          -2.5
        ]
      },
      "returns": 3
    },
    {
      "arguments": {
        "data": [
          1005.5,
          -5.7,
          -3.575,
        ]
      },
      "returns": 332.075
    },
    {
      "arguments": {
        "data": [
          -Infinity,
          3,
          5
        ]
      },
      "returns": -Infinity
    },
    {
      "arguments": {
        "data": [
          3,
          5,
          Infinity
        ]
      },
      "returns": Infinity
    },
    {
      "arguments": {
        "data": [
          -Infinity,
          3,
          3,
          5,
          Infinity
        ]
      },
      "returns": NaN
    },
    {
      "arguments": {
        "data": [
          NaN,
          3,
          3,
          NaN,
          5
        ]
      },
      "returns": NaN
    },
    {
      "arguments": {
        "data": [
          1,
          2,
          null
        ],
        "ignore_nodata": false
      },
      "returns": null
    },
    {
      // The input array is empty: return `null`.
      "arguments": {
        "data": []
      },
      "returns": null
    },
    {
      // The input array has only `null` elements: return `null`.
      "arguments": {
        "data": [
          null,
          null
        ]
      },
      "returns": null
    },
    {
      "arguments": {
        "data": [
          null,
          null
        ],
        "ignore_nodata": false
      },
      "returns": null
    }
  ]
}
