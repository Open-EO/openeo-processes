      "arguments": {
        "data": [
          {"type": "nodata"},
          -2,
          -2.0,
          -2,
          -2.0
        ]
      },
      "returns": 16
    },
    {
      "arguments": {
        "data": [
          1,
          {"type": "nodata"}
        ],
        "ignore_nodata": false
      },
      "returns": {"type": "nodata"}
    },
    {
      "arguments": {
        "data": [
          -1
        ]
      },
      "returns": -1
    },
    {
      "arguments": {
        "data": [
          {"type": "nodata"}
        ],
        "ignore_nodata": false
      },
      "returns": {"type": "nodata"}
    },
    {
      "arguments": {
        "data": []
