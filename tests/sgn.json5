    {
      "arguments": {
        "x": {"type": "nodata"}
      },
      "returns": {"type": "nodata"}
    },
    {
      "arguments": {
        "x": NaN
      },
      "returns": NaN
    },
    {
      "arguments": {
        "x": Infinity
      },
      "returns": 1
    },
    {
      "arguments": {
        "x": -Infinity
      },
      "returns": -1
    },
  ]
