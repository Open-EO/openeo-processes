{
  "id": "first",
  "level": "L1",
  "tests": [
    {
      "arguments": {
        "data": [
          1,
          0,
          3,
          2
        ]
      },
      "returns": 1
    },
    {
      "arguments": {
        "data": [
          null,
          "A",
          "B"
        ]
      },
      "returns": "A"
    },
    {
      "arguments": {
        "data": [
          null,
          2,
          3
        ],
        "ignore_nodata": false
      },
      "returns": null
    },
    {
      "arguments": {
        "data": [
          null,
          null,
          null,
          -1
        ]
      },
      "returns": -1
    },
    {
      "arguments": {
        "data": [
          null,
          null
        ]
      },
      "returns": null
    },
    {
      // The input array is empty: return `null`.
      "arguments": {
        "data": []
      },
      "returns": null
    },
    {
      "arguments": {
        "data": [
          NaN,
          1,
          2,
          3
        ]
      },
      "returns": NaN
    }
  ]
}
