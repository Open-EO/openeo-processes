{
  "id": "rearrange",
  "level": "L3",
  "tests": [
    {
      // Reverse a list
      "arguments": {
        "data": [
          5,
          4,
          3
        ],
        "order": [
          2,
          1,
          0
        ]
      },
      "returns": [
        3,
        4,
        5
      ]
    },
    {
      // Remove two elements
      "arguments": {
        "data": [
          5,
          4,
          3,
          2
        ],
        "order": [
          1,
          3
        ]
      },
      "returns": [
        4,
        2
      ]
    },
    {
      // Swap two elements
      "arguments": {
        "data": [
          5,
          4,
          3,
          2
        ],
        "order": [
          0,
          2,
          1,
          3
        ]
      },
      "returns": [
        5,
        3,
        4,
        2
      ]
    }
  ]
}
