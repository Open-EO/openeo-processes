{
  "id": "date_difference",
  "level": "L2-Date",
  "experimental": true,
  "tests": [
    {
      "arguments": {
        "date1": "2020-01-01T00:00:00.0Z",
        "date2": "2020-01-01T00:00:15.5Z"
      },
      "returns": 15.5
    },
    {
      "arguments": {
        "date1": "2020-01-01T00:00:00Z",
        "date2": "2020-01-01T01:00:00+01:00"
      },
      "returns": 0
    },
    {
      "arguments": {
        "date1": "2020-01-02",
        "date2": "2020-01-01"
      },
      "returns": -86400
    },
    {
      "arguments": {
        "date1": "2020-01-02",
        "date2": "2020-01-01",
        "unit": "day"
      },
      "returns": -1
    }
  ]
}
